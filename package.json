    "server",
    "http",
    "request"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.5",
    "@types/formidable": "^1.0.31",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.1",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
